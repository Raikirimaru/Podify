{
    "installCommand": "pnpm i --legacy-peer-deps",
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"] }
        },
        {
            "src": "src/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}